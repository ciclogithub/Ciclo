@{
    ViewBag.Title = "Cadastro";
}

<div class="tg-bgparallax">
    <div class="container">
        <div class="tg-innerbannercontent">
            <div class="tg-pagetitle">
                <h1>CADASTRO</h1>
            </div>
            <ol class="tg-breadcrumb">
                <li><a href="#">Home</a></li>
                <li class="tg-active">Cadastro</li>
            </ol>
        </div>
    </div>
</div>

<main id="tg-main" class="tg-main tg-haslayout">
    <div class="tg-sectionspace">
        <div class="container">
            <div class="row">
                <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
                    <form class="tg-formtheme tg-formcontactus" id="form_cadastro_empresa">
                        <fieldset>
                            <div class="form-group">
                                <input type="text" name="txempresa" id="txempresa" class="form-control" placeholder="Nome da Empresa/Instrutor *">
                            </div>
                            <div class="form-group">
                                <input type="text" name="txemail" id="txemail" class="form-control" placeholder="E-mail *">
                            </div>
                            <div class="form-group">
                                <input type="text" name="txtelefone" id="txtelefone" class="form-control" placeholder="Telefone *">
                            </div>
                            <div class="form-group">
                                <input type="password" name="txsenha" id="txsenha" class="form-control" placeholder="Senha *">
                            </div>
                            <div class="form-group">
                                <textarea name="txdescritivo" id="txdescritivo" placeholder="Descrição"></textarea>
                            </div>
                            <div class="form-group">
                                <button class="tg-btn" type="button" id="btn_cadastro_empresa">Cadastrar</button>
                            </div>
                            <div class="form-group">
                                <div id="form-error" class="p10"></div>
                            </div>
                        </fieldset>
                    </form>
                    
                </div>
            </div>
        </div>
    </div>
</main>