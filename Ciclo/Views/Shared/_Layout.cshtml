<html class="no-js" lang="">
 <!--<![endif]-->
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>Treina Auto - A 1º Plataforma de Eventos Automotivos</title>
    <meta name="description" content="">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="apple-touch-icon" href="apple-touch-icon.png">

    <link rel="stylesheet" type="text/css" href="~/Content/bootstrap.css?1" />
    <link rel="stylesheet" type="text/css" href="~/Content/site.css?1" />
    <link rel="stylesheet" type="text/css" href="~/Content/normalize.css?1" />
    <link rel="stylesheet" type="text/css" href="~/Content/font-awesome.min.css?1" />
    <link rel="stylesheet" type="text/css" href="~/Content/icomoon.css?1" />
    <link rel="stylesheet" type="text/css" href="~/Content/customScrollbar.css?1" />
    <link rel="stylesheet" type="text/css" href="~/Content/animate.css?1" />
    <link rel="stylesheet" type="text/css" href="~/Content/jquery.fullpage.css?1" />
    <link rel="stylesheet" type="text/css" href="~/Content/transitions.css?1" />
    <link rel="stylesheet" type="text/css" href="~/Content/main.css?1" />
    <link rel="stylesheet" type="text/css" href="~/Content/color.css?1" />
    <link rel="stylesheet" type="text/css" href="~/Scripts/highcharts/5.0.14/css/highcharts.css?1" />
    <link rel="stylesheet" type="text/css" href="~/Scripts/plugins/barrating/themes/fontawesome-stars.css?1" />
    <link rel="stylesheet" type="text/css" href="~/Scripts/plugins/validate/css/validationEngine.jquery.css?1" />
    <link rel="stylesheet" type="text/css" href="~/Scripts/plugins/calendar/css/datepicker.css?1" />
    <link rel="stylesheet" type="text/css" href="~/Scripts/plugins/select2/css/select2.css?1" />
    <link rel="stylesheet" type="text/css" href="~/Content/responsive.css?1" />
    <link rel="stylesheet" type="text/css" href="//cdn.jsdelivr.net/bootstrap.daterangepicker/2/daterangepicker.css" />

    @*@Styles.Render("~/Content/css")
    @Styles.Render("~/Content/template")*@
    @Scripts.Render("~/bundles/modernizr")
</head>
<body class="tg-home tg-homevtwo">

    <div id="tg-wrapper" class="tg-wrapper tg-haslayout">

        <header id="tg-header" class="tg-header tg-headervtwo tg-haslayout">
            <div class="container-fluid">
                <div class="row">
                    <div class="col-xs-6 col-sm-6 col-md-6 col-lg-6">
                        <strong class="tg-logo"><a href="/"><img src="~/images/logo.png" class="img-responsive" alt="www.treinaauto.com.br"></a></strong>
                    </div>
                    <div class="col-xs-6 col-sm-6 col-md-6 col-lg-6">
                        @Html.Action("Menu", "Home")
                    </div>
                </div>
            </div>
        </header>

        <form action="~/Pesquisa" method="post" id="filtro_form" name="filtro_form">
            <div class="pesquisa">
                <div class="container">
                    <div class="row">
                        <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12 text-center">
                            <h3>1º Plataforma de treinamento automotivo do Brasil. <strong>Encontre seu próximo curso aqui!</strong></h3>
                        </div>
                    </div>
                    <div class="row pt30">
                        <div class="col-xs-12 col-sm-12 col-md-3 col-lg-3 text-center">
                            <div id="lupa"></div>
                            <input type="text" id="filtro_curso" name="filtro_curso" placeholder="Pesquisar curso" />
                        </div>
                        <div class="col-xs-12 col-sm-12 col-md-3 col-lg-3 text-center">
                            <div id="mapa"></div>
                            <input type="text" id="filtro_cidade" name="filtro_cidade" placeholder="Pesquisar por cidade" />
                        </div>
                        <div class="col-xs-12 col-sm-12 col-md-3 col-lg-3 text-center">
                            <div id="calendar"></div>
                            <input readonly type="text" id="filtro_data" name="filtro_data" placeholder="Todas as datas" />
                        </div>
                        <div class="col-xs-12 col-sm-12 col-md-3 col-lg-3 text-center">
                            <input type="button" id="filtro_pesquisar" name="filtro_pesquisar" value="Pesquisar" />
                        </div>
                    </div>
                </div>
            </div>
        </form>

        @RenderBody()

        <footer id="tg-footer" class="tg-haslayout">
            <div class="tg-foorterbar">
                <div class="container">
                    <div class="row">
                        <div class="col-xs-12 col-sm-4 col-md-3 col-lg-3">
                            <p class="tg-copyrights">@DateTime.Now.Year - Todos os direitos reservados</p>
                        </div>
                        <div class="col-xs-12 col-sm-4 col-md-6 col-lg-6 text-center">
                            <strong class="tg-logo2"><a href="~/"><img src="~/images/logo_branca.png" alt="www.treinaauto.com.br"></a></strong>
                        </div>
                        <div class="col-xs-12 col-sm-4 col-md-3 col-lg-3">
                            <ul class="tg-socialicons">
                                <li class="tg-facebook"><a href="https://www.facebook.com/treinaautocursos" target="_blank"><i class="fa fa-facebook"></i></a></li>
                                @*<li class="tg-twitter"><a href="javascript:void(0);"><i class="fa fa-twitter"></i></a></li>
                                <li class="tg-linkedin"><a href="javascript:void(0);"><i class="fa fa-linkedin"></i></a></li>
                                <li class="tg-googleplus"><a href="javascript:void(0);"><i class="fa fa-google-plus"></i></a></li>
                                <li class="tg-rss"><a href="javascript:void(0);"><i class="fa fa-rss"></i></a></li>*@
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
        </footer>

    </div>            

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/bundles/template")
    @Scripts.Render("~/bundles/site")
    @RenderSection("scripts", required: false)

</body>
</html>

<script type="text/javascript" src="//cdn.jsdelivr.net/momentjs/latest/moment.min.js"></script>
<script type="text/javascript" src="//cdn.jsdelivr.net/bootstrap.daterangepicker/2/daterangepicker.js"></script>

